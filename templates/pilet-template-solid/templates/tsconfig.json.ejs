{
  "compilerOptions": {
    "target": "es6",
    "sourceMap": true,
    "skipLibCheck": true,
    "lib": ["dom", "es2018"],
    "moduleResolution": "Bundler",
    "module": "esnext",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "importHelpers": true
  },
  "include": [
    "<%- src %>"
  ],
  "exclude": [
    "node_modules"
  ]
}
